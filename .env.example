# Copy this file to .env and fill in your values

# Required
GEMINI_API_KEY=your_gemini_api_key_here

# Required — Neon Postgres connection string
# Get this from your Neon dashboard: https://console.neon.tech
DATABASE_URL=postgresql://user:password@ep-example-123456.us-east-2.aws.neon.tech/dbname?sslmode=require

# Optional — defaults shown
GEMINI_MODEL=gemini-2.5-flash

# Rate limiting
RATE_LIMIT_PER_IP=5/hour
DAILY_ANALYSIS_CAP=150
